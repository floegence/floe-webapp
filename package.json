{
  "name": "floe-webapp",
  "version": "0.1.0",
  "private": true,
  "description": "A professional VSCode-style web application framework",
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev.mjs",
    "build": "pnpm -r build",
    "build:demo": "pnpm --filter @floegence/floe-webapp-core build && pnpm --filter @floegence/floe-webapp-protocol build && NODE_OPTIONS=--max-old-space-size=4096 pnpm --filter @floegence/floe-webapp-demo build",
    "verify": "node scripts/verify-dist.mjs",
    "lint": "eslint packages apps scripts --ext .ts,.tsx,.mjs",
    "format": "prettier --write \"**/*.{ts,tsx,css,json,md}\"",
    "typecheck": "pnpm -r typecheck",
    "test": "vitest",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-solid": "^0.14.5",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
