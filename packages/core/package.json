{
  "name": "@floegence/floe-webapp-core",
  "version": "0.9.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/styles.css",
    "./tailwind": "./dist/tailwind.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node ./scripts/dev.mjs",
    "build": "pnpm clean && vite build && tsc -p tsconfig.build.json && pnpm build:styles && pnpm build:assets",
    "build:assets": "node ./scripts/copy-style-assets.mjs",
    "build:styles": "vite build -c vite.styles.config.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "diff": "^8.0.3",
    "marked": "^17.0.1",
    "mermaid": "^11.12.2",
    "shiki": "^3.21.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "solid-js": "^1.9.11",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-solid": "^2.11.10"
  }
}
